{
  "name": "@jkzasori/jkicons",
  "version": "1.0.3",
  "description": "Librería de íconos SVG personalizables creada por José Támara.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "files": [
    "dist",
    "react",
    "icons"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react/*": "./react/*",
    "./icons/*": "./icons/*"
  },
  "keywords": [
    "icons",
    "svg",
    "react",
    "jkicons",
    "icon-library",
    "components"
  ],
  "author": "José Támara <jkzasori@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jkzasori/jkicons"
  },
  "homepage": "https://github.com/jkzasori/jkicons#readme",
  "bugs": {
    "url": "https://github.com/jkzasori/jkicons/issues"
  },
  "scripts": {
    "svgs:clean": "rimraf src/react && rimraf dist-icons",
    "svgs:build:react": "node ./scripts/build-svgs-to-react.js",
    "build": "npm run svgs:build:react && tsup",
    "dev": "tsup --watch",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs/.vitepress/dist",
    "zip": "mkdir -p releases && (cd dist && zip -r ../releases/jkicons-v$(node -p \"require('../package.json').version\") .)",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "jkicon": "./bin/jkicon.js"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@svgr/cli": "^6.5.1",
    "@svgr/core": "^8.1.0",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "svgo": "^3.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.6.3",
    "vitepress": "^1.3.4"
  }
}
